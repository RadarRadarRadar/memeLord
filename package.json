{
  "name": "memelord",
  "version": "1.0.0",
  "description": "a meme lord bot for discord",
  "main": "app.js",
  "scripts": {
    "lint": "eslint --fix --ext .ts ./src",
    "compile": "npm run lint && rm -rf dist/ && tsc",
    "start": "npm run compile && node --enable-source-maps dist/app.js",
    "start:watch": "nodemon",
    "docker-compose:up": "docker-compose up --build",
    "migrate": "sequelize-cli db:migrate --env prod",
    "seed": "sequelize-cli db:seed:all --env prod"
  },
  "author": "harold waters",
  "license": "ISC",
  "devDependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/node": "^13.9.0",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-standard-with-typescript": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "canvas": "^2.6.1",
    "discord.js": "^12.0.2",
    "nodemon": "^2.0.2",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.1.1",
    "wordpos": "^2.0.0"
  }
}
